{
  "version": "4.9.0",
  "description": "Sendbird SDK for JavaScript",
  "name": "@sendbird/chat",
  "author": "Sendbird <support@sendbird.com>",
  "keywords": [
    "sendbird",
    "sendbird.com",
    "messaging",
    "chat",
    "js"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://sendbird.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sendbird/sendbird-chat-sdk-javascript"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6"
  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": {
      "optional": true
    }
  },
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "react-native": "./index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./message": {
      "types": "./message.d.ts",
      "default": "./message.js"
    },
    "./groupChannel": {
      "types": "./groupChannel.d.ts",
      "default": "./groupChannel.js"
    },
    "./openChannel": {
      "types": "./openChannel.d.ts",
      "default": "./openChannel.js"
    },
    "./poll": {
      "types": "./poll.d.ts",
      "default": "./poll.js"
    }
  },
  "typesVersions": {
    "*": {
      "message": [
        "./message.d.ts"
      ],
      "groupChannel": [
        "./groupChannel.d.ts"
      ],
      "openChannel": [
        "./openChannel.d.ts"
      ],
      "poll": [
        "./poll.d.ts"
      ]
    }
  }
}